#lang crook
{:= A B C D0 D0.A D1 E0 E1 F H0 H1 I J K}
(require "../compile.rkt")
(require "../parse.rkt")
(require "../run.rkt")
(require "test-runner.rkt")

{:> A I}
(test (位 (e) (run (compile (parse e)))))
{:> I}
(test (位 p (run (compile (apply parse p)))))

{:> E0 I}
(test/io (位 (in e) (run/io (compile (parse e)) in)))
